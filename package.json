{
  "name": "NeutronStudio",
  "productName": "NeutronStudio",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Una app suite, capaz de crear recursos de manera rapida y avanzada, con caracteristicas unicas.",
  "author": "StemDev0110 <virusstem.contact@gmail.com>",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build-lnx": "tsc && vite build && electron-builder build --linux",
    "build-mac": "tsc && vite build && electron-builder build --mac",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "asar": "^3.2.0",
    "axios": "^1.5.1",
    "cross-env": "^7.0.3",
    "discord-rpc": "^4.0.1",
    "electron-forge": "^5.2.4",
    "electron-updater": "^6.1.4",
    "firebase": "^10.5.0",
    "minecraft-launcher-core": "^3.17.2",
    "mineflayer": "^4.14.0",
    "msmc": "^4.1.0",
    "react-router-dom": "6.21.1",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@types/discord-rpc": "^4.0.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.16",
    "electron": "^26.0.0",
    "electron-builder": "^24.6.3",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.13.0-beta.3",
    "vite-plugin-electron-renderer": "^0.14.5"
